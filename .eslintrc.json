{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "eslint:recommended",
      "airbnb"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  "react/button-has-type": 0,
  "no-underscore-dangle": 0,
  "comma-dangle": 0,
  "react/prop-types": "warn",
  "indent": [
    "error",
    2
  ],
  "linebreak-style": [
    "error",
    "unix"
  ],
  "quotes": [
    "error",
    "single"
  ],
  "semi": [
    "error",
    "always"
  ],
  "no-useless-return": 1,
  "class-methods-use-this": 0
}
}