{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "flutterwave-react-v3": "^1.2.0",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^16.2.0",
    "react-to-pdf": "^0.0.14",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0",
    "redux-thunk": "^2.3.0",
    "uuid": "^7.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --coverage ",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "enzyme": "^3.11.0",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "postcss-normalize": "^10.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js",
      "\\.(css|less)$": "<rootDir>/fileTransformer.js",
      "\\.svg": "<rootDir>/svgMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "html",
      [
        "text",
        {
          "subdir": "./coverage"
        }
      ]
    ]
  },
  "peerDependencies": {
    "browserslist": "^4.19.1",
    "postcss": "^8.4.6",
    "typescript": "^4.5.5"
  },
  "optionalDependencies": {
    "canvg": "^4.0.0",
    "dompurify": "^2.3.8",
    "html2canvas": "^1.4.1"
  }
}
